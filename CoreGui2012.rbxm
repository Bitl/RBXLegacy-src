&#9;&#9;&#9;local textChangers
&#9;&#9;&#9;teamObject, statObject, textChangers = createStatColumns(teamObject, numStatColumns, true, suffix == &quot;Big&quot;)
&#9;&#9;&#9;teamTable[team][&quot;StatObject&quot; .. suffix] = statObject
